{
  "name": "realm",
  "version": "1.0.0",
  "description": "Runtime-editable elm.",
  "main": "dist/electron.js",
  "repository": "https://github.com/inkandswitch/realm",
  "author": "Jeff Peterson <jeff@yak.sh>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently 'yarn server' 'sleep 3 && yarn start-app'",
    "server": "node -r ts-node/register ./src/server",
    "start-app": "yarn build --config-name=electron && electron .",
    "build": "webpack",
    "app": "yarn build && yarn start-app",
    "types": "./bin/types.sh",
    "counter": "yarn build --config-name=counter && node dist/counter.js",
    "cat": "yarn build --config-name=doccat && node dist/doccat.js",
    "realm": "cross-env REPO_ROOT=./.data/bots node ./dist/realm.js",
    "bot": "yarn realm bot"
  },
  "devDependencies": {
    "@types/bs58": "^3.0.30",
    "@types/commander": "^2.12.2",
    "@types/debug": "^0.0.31",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.16.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.0",
    "@types/proper-lockfile": "^3.0.0",
    "@types/webpack": "^4.4.17",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-node-externals": "^1.6.3",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^3.0.7",
    "elm-webpack-loader": "^5.0.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.14.3",
    "prettier-plugin-elm": "^0.4.1",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "automerge": "^0.9.2",
    "bs58": "^4.0.1",
    "commander": "^2.19.0",
    "dat-swarm-defaults": "^1.0.1",
    "debug": "^4.1.0",
    "discovery-swarm": "^5.1.2",
    "electron-context-menu": "^0.10.1",
    "hypercore": "^6.21.0",
    "hypermerge": "github:automerge/hypermerge#fork",
    "lodash": "^4.17.11",
    "node-elm-compiler": "^5.0.1",
    "odiff": "github:inkandswitch/odiff",
    "proper-lockfile": "^3.2.0",
    "random-access-file": "^2.0.1",
    "random-access-memory": "^3.0.0",
    "tiny-worker": "^2.1.2"
  }
