{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/node_modules/@types"],
    "sourceMap": true,
    "lib": ["es2015", "dom", "webworker"],
    "module": "commonjs",
    "target": "es5",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true
  }
}
